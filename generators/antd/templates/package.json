{
  "name": "antdv",
  "version": "0.1.0",
  "scripts": {
    "dev": "vue-cli-service serve --mode dev",
    "build:test": "vue-cli-service build --mode test --report",
    "build:prod": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "inspect:prod": "vue-cli-service inspect --mode prod > prod.catWebpackConfig.js",
    "inspect:dev": "vue-cli-service inspect --mode dev > dev.catWebpackConfig.js"
  },
  "dependencies": {
    "ant-design-vue": "^1.6.2",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "dingtalk-jsapi": "^2.11.10",
    "js-file-download": "^0.4.11",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "qs": "^6.9.3",
    "quill": "^1.3.7",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-plugin-vuex": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "compression-webpack-plugin": "^3.1.0",
    "css-generator-plugin": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "import-assets-from-cdn": "^0.0.2",
    "less": "^3.11.3",
    "less-loader": "^6.1.0",
    "lint-staged": "^9.5.0",
    "log-info-webpack-plugin": "^1.2.1",
    "typescript": "^3.9.5",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
